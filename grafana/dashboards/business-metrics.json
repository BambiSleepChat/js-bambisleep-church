{
  "dashboard": {
    "title": "BambiSleep Church - Business Metrics & Analytics",
    "tags": [
      "business",
      "analytics",
      "content",
      "users"
    ],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "New User Registrations (24h)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "increase(user_registrations_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "blue"
                },
                {
                  "value": 5,
                  "color": "green"
                },
                {
                  "value": 20,
                  "color": "yellow"
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Video Streams (24h)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "increase(video_streams_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "blue"
                },
                {
                  "value": 10,
                  "color": "green"
                },
                {
                  "value": 50,
                  "color": "yellow"
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Content Access (24h)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(increase(content_access_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "blue"
                },
                {
                  "value": 50,
                  "color": "green"
                },
                {
                  "value": 200,
                  "color": "yellow"
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Premium vs Public Content Ratio",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(increase(content_access_total{access_level=\"premium\"}[24h])) / sum(increase(content_access_total[24h])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 30,
                  "color": "yellow"
                },
                {
                  "value": 60,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "User Registration Trend",
        "type": "graph",
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 6
        },
        "targets": [
          {
            "expr": "increase(user_registrations_total[1h])",
            "legendFormat": "Registrations/hour",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "New Users"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "title": "Content Access by Type",
        "type": "graph",
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 6
        },
        "targets": [
          {
            "expr": "sum(rate(content_access_total[5m])) by (content_type)",
            "legendFormat": "{{content_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Access/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "title": "Content Access by Level",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(increase(content_access_total[24h])) by (access_level)",
            "legendFormat": "{{access_level}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "display": true,
            "placement": "right"
          }
        }
      },
      {
        "id": 8,
        "title": "Video Stream Duration (p95)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(video_stream_duration_bucket[5m]))",
            "legendFormat": "p95 Duration",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(video_stream_duration_bucket[5m]))",
            "legendFormat": "p50 Duration",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 9,
        "title": "Video Streams by Video ID",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        },
        "targets": [
          {
            "expr": "sum(rate(video_streams_total[5m])) by (video_id)",
            "legendFormat": "{{video_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Streams/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "Content Type Distribution (7 days)",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(increase(content_access_total[7d])) by (content_type)",
            "legendFormat": "{{content_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "display": true,
            "placement": "right"
          }
        }
      },
      {
        "id": 11,
        "title": "Premium Content Engagement",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "sum(rate(content_access_total{access_level=\"premium\",content_type=\"markdown\"}[5m]))",
            "legendFormat": "Premium Markdown",
            "refId": "A"
          },
          {
            "expr": "sum(rate(content_access_total{access_level=\"premium\",content_type=\"video\"}[5m]))",
            "legendFormat": "Premium Video",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Access/sec"
          },
          {
            "format": "short"
          }
        ]
      }
    ]
  }
}